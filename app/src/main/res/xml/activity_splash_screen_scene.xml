<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1500"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet>

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/circle"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/text"
            android:layout_marginBottom="75dp"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.54" />
        <Constraint
            android:id="@+id/farmer"
            android:layout_width="300dp"
            android:layout_height="270dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="180dp"
            android:layout_marginStart="55dp"></Constraint>
        <Constraint
            android:id="@+id/tangan"
            android:layout_width="300dp"
            android:layout_height="270dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="240dp"
            android:layout_marginStart="20dp"
            android:rotation="-35"></Constraint>
        <Constraint
            android:layout_height="61dp"
            android:layout_width="95dp"
            android:id="@+id/tomate"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="188dp"
            android:layout_marginLeft="60dp"
            android:layout_marginStart="60dp" />
        <Constraint
            android:layout_width="95dp"
            android:layout_height="80dp"
            android:rotation="55"
            android:id="@+id/lobak"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="56dp"
            android:layout_marginRight="56dp"
            android:layout_marginTop="224dp" />
        <Constraint
            android:layout_width="95dp"
            android:layout_height="61dp"
            android:rotation="25"
            android:id="@+id/carrot"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="252dp"
            android:layout_marginLeft="60dp"
            android:layout_marginStart="60dp" />
        <Constraint
            android:id="@+id/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/farmer"
            android:layout_marginLeft="16dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible"
            android:layout_marginStart="16dp"
            android:layout_marginTop="68dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/circle"
            android:layout_width="300dp"
            android:layout_height="300dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/text"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.54" />
        <Constraint
            android:id="@+id/tangan"
            android:layout_width="300dp"
            android:layout_height="270dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="180dp"
            android:layout_marginStart="55dp"></Constraint>
        <Constraint
            android:id="@+id/farmer"
            android:layout_width="300dp"
            android:layout_height="270dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="180dp"
            android:layout_marginStart="55dp"></Constraint>
        <Constraint
            android:layout_height="61dp"
            android:layout_width="95dp"
            android:id="@+id/tomate"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="156dp"
            android:layout_marginLeft="60dp"
            android:layout_marginStart="60dp" />
        <Constraint
            android:layout_height="61dp"
            android:layout_width="95dp"
            android:id="@+id/carrot"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="236dp"
            android:layout_marginLeft="28dp"
            android:layout_marginStart="28dp" />
        <Constraint
            android:layout_height="80dp"
            android:layout_width="95dp"
            android:id="@+id/lobak"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="176dp"
            android:layout_marginLeft="280dp"
            android:layout_marginStart="280dp" />
    </ConstraintSet>
</MotionScene>